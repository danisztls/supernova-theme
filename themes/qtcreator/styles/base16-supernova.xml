<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base16 Supernova">
  <!--
  base16-qtcreator (https://github.com/ilpianista/base16-qtcreator)
  by Chris Kempson (http://chriskempson.com)
  Supernova scheme by @lbcnz (https://github.com/lbcnz/supernova-theme)
  -->

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#cdd9e5" background="#2d333b"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#545d68"/>
  <style name="CurrentLine" background="#909dab"/>
  <style name="LineNumber" foreground="#909dab"/>
  <style name="CurrentLineNumber" foreground="#adbac7" bold="true"/>
  <style name="SearchResult" background="#373e47"/>
  <style name="Parentheses" background="#545d68" />
  <style name="ParenthesesMismatch" background="#00af9e" />

  <!-- Selection whilst searching in it -->
  <style name="SearchScope" foreground="#ffffff" background="#909dab"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#909dab"/>
  <style name="Occurrences.Rename" background="#adbac7"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#ff5c00"/>

  <!-- language primitives -->
  <style name="Number" foreground="#ff5c00"/>
  <style name="String" foreground="#6bac00"/>
  <style name="Keyword" foreground="#00af9e"/>
  <style name="Operator" foreground="#cdd9e5"/>
  <style name="Preprocessor" foreground="#237ef4"/>
  <style name="Label" foreground="#d40059"/>

  <!-- types, classes -->
  <style name="Type" foreground="#d9b600"/>
  <style name="PrimitiveType" foreground="#00af9e"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#237ef4"/>
  <style name="VirtualMethod" foreground="#237ef4" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#e7f3f4"/>
  <style name="Local" foreground="#cdd9e5"/>
  <style name="Global" foreground="#cdd9e5"/>
  <style name="Static" foreground="#cdd9e5"/>
  <style name="JsScopeVar" foreground="#cdd9e5" italic="true"/>
  <style name="JsImportVar" foreground="#cdd9e5" italic="true"/>
  <style name="JsGlobalVar" foreground="#cdd9e5" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#545d68"/>

  <!-- comments -->
  <style name="Comment" foreground="#909dab"/>
  <style name="DisabledCode" foreground="#909dab"/>
  <style name="Doxygen.Comment" foreground="#909dab"/>
  <style name="Doxygen.Tag" foreground="#909dab"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#00af9e"/>
  <style name="Binding" foreground="#d40059"/>
  <style name="QmlExternalId" foreground="#237ef4" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#237ef4" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#00af9e"/>
  <style name="DiffLocation" foreground="#237ef4"/>
  <style name="AddedLine" foreground="#6bac00"/>
  <style name="RemovedLine" foreground="#d9b600"/>
  <style name="LogChangeLine" foreground="#d40059"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#00af9e"/>
  <style name="DiffContextLine" foreground="#237ef4"/>
  <style name="DiffSourceLine" foreground="#6bac00"/>
  <style name="DiffSourceChar" foreground="#d9b600" background="#373e47"/>
  <style name="DiffDestLine" foreground="#6bac00"/>
  <style name="DiffDestChar" foreground="#d9b600" background="#373e47"/>

  <!-- errors, warnings -->
  <style name="Error" underlineColor="#d40059" underlineStyle="SingleUnderline"/>
  <style name="ErrorContext" underlineColor="#d40059" underlineStyle="DotLine"/>
  <style name="Warning" underlineColor="#d9b600" underlineStyle="SingleUnderline"/>
  <style name="WarningContext" underlineColor="#d9b600" underlineStyle="DotLine"/>
</style-scheme>
